	<template>
		<name>Turret</name>
		<parameter>const int id, const int type_turret, const int coord_x_turret, const int coord_y_turret</parameter>
		<declaration>void bang(int i) {
    int j;
    bool boom=false; 
    enemy_health[i] = enemy_health[i] - TURRETS[type_turret].inflicted_damage; // shoot the enemy
    if (enemy_health[i] &lt;= 0) { // teleport the enemy outside the map
        alive[i]= false;
        for (j = 0; j &lt; N_ENEMIES &amp;&amp; boom==true; j++) { 
            if(alive[j]){
                boom=true;
            }
          }
        if(!boom) { //se i=6 vuol dire che non sono uscita dal for a causa del break, ma perchè ho scorso tutto l'array
            end_game=true;
        }

    }
}

int max(int a, int b) {
    return a &gt; b ? a : b;
}

bool shoot_enemy() {

    // codice che decide a chi sparare (in: coordinate sue+tutti nemici, raggio, output: int quale nemico)
    // codice che spara a int e ritorna true se ha sparato (in: danno, int quale nemico, out: bool)

    enemy_int enemy_to_shoot; // which enemy to shoot (if in range)
    int closest_distance = 1000; // 1000 means no enemy in range
    int temp_distance;
    int i;
    for (i = 0; i &lt; N_ENEMIES; i++) { //scorre tutto l'array e trova il nemico piu vicino alla torretta a cui sparare
      if(alive[i]){ //è vero e il nemico è vivo
        temp_distance = max(abs(coord_x[i] - coord_x_turret), abs(coord_y[i] - coord_y_turret));
        if (temp_distance &lt;= TURRETS[type_turret].shooting_range &amp;&amp; temp_distance &lt; closest_distance) { // will shoot to smallest id //aggiorniamo la closest distance solo se entra nel range di attacco della torertta
            closest_distance = temp_distance;
            enemy_to_shoot = i;
            if (closest_distance == 1) { // shoot if closest_distance is 1: optimization!
                bang(enemy_to_shoot);
                return true;
            }
        }
    }
    }

    if (closest_distance &lt; 1000) { // means I found an enemy in range --- spara effettivamnete al nemico se la sua distanza dalla torretta è maggiore di 1
        bang(enemy_to_shoot);
        return true;
    }
    return false;
}

bool just_shot = false;
